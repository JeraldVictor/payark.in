<% include ../layouts/header.ejs %><link rel="stylesheet" href="/stylesheets/transactions.css"><div class="container">    <div class="headding">        <h1 class="text-center">Transactions</h1>    </div>    <div class="row text-center">        <div class="col">            <a href="/home" class="btn btn-dark">Home</a>        </div>        <div class="col">            <a href="/receipts" class="btn btn-dark">Receipts</a>        </div>    </div>    <table class="table table-striped table-dark">        <thead>        <tr>            <th scope="col">SI.NO</th>            <th scope="col">ID</th>            <th scope="col">Date [YYYY-MM-YY]</th>            <th scope="col">Purpose</th>            <th scope="col">Amount Transfered</th>        </tr>        </thead>        <tbody>        <%var si=1; list.forEach(function (i) {%>        <tr>            <th scope="row"><%=si%></th>            <td><%=i.t_id%></td>            <td><%=i.t_date%></td>            <td><%=i.t_purpose%></td>            <td>&#8377; <%=i.t_amout%></td>        </tr>        <%si++; });%>        </tbody>    </table></div>
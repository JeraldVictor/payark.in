<% include ../layouts/header.ejs %><link rel="stylesheet" href="/stylesheets/user.css"><div class="container">    <br><br>    <div class="headding">        <h1 class="text-center">Your Details</h1>    </div>    <div class="text-center container" id="container">        <div class="form-row">            <div class="col-md-4 mb-3">                <label for="validationServer01">First name</label>                <input type="text" class="form-control is-valid" id="validationServer01" placeholder="First name" value="<%=you.fname%>" disabled>            </div>            <div class="col-md-4 mb-3">                <label for="validationServer02">Last name</label>                <input type="text" class="form-control is-valid" id="validationServer02" placeholder="Last name" value="<%=you.lname%>" disabled>            </div>        </div>        <div class="form-row">            <div class="col-md-4 mb-3">                <label for="validationServer01">E-mail</label>                <input type="text" class="form-control is-valid" id="validationServer01" placeholder="First name" value="<%=you.email%>" disabled>            </div>            <div class="col-md-4 mb-3">                <label for="validationServer02">Phone Number</label>                <input type="text" class="form-control is-valid" id="validationServer02" placeholder="Last name" value="<%=you.phno%>" disabled>            </div>        </div>        <div class="form-row">            <div class="col-md-4 mb-3">                <label for="validationServer01">Gender</label>                <input type="text" class="form-control is-valid" id="validationServer01" placeholder="First name" value="<%=you.gender%>" disabled>            </div>            <div class="col-md-4 mb-3">                <label for="validationServer02">Phone Number</label>                <input type="text" class="form-control is-valid" id="validationServer02" placeholder="Last name" value="<%=you.dob%>" disabled>            </div>        </div>    </div>    <div class="text-center">        <span class="badge badge-pill badge-warning">You Can Not Modify Your Details for Time Being</span>    </div></div><% include ../layouts/footer.ejs %>